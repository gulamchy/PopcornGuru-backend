{
  "version": 2,
  "name": "PopcornGuru-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://popcorn-guru-frontend.vercel.app"
        },
        { "key": "Access-Control-Allow-Methods", "value": "POST,GET,OPTIONS" }
      ]
    }
  ]
}
